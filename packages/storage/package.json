{
  "name": "@ictirc/storage",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./r2": {
      "types": "./src/r2/index.ts",
      "default": "./src/r2/index.ts"
    },
    "./supabase": {
      "types": "./src/supabase/index.ts",
      "default": "./src/supabase/index.ts"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.525.0",
    "@aws-sdk/s3-request-presigner": "3.525.0",
    "@supabase/supabase-js": "2.49.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@ictirc/config": "workspace:*",
    "@types/node": "22.10.7",
    "typescript": "5.7.3"
  }
}
